#!/usr/bin/make -f

srctree ?= .

build-indep:
build-arch:
	$(MAKE) KERNELRELEASE=6.1.141 ARCH=arm64 	KBUILD_BUILD_VERSION=5 -f $(srctree)/Makefile

build: build-arch

binary-indep:
binary-arch: build-arch
	$(MAKE) KERNELRELEASE=6.1.141 ARCH=arm64 	KBUILD_BUILD_VERSION=5 -f $(srctree)/Makefile intdeb-pkg

clean:
	rm -rf debian/files debian/linux-*
	$(MAKE) clean

binary: binary-arch
